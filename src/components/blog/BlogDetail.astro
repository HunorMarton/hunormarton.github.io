---
import { type CollectionEntry, render } from "astro:content";

interface Props {
  entry: CollectionEntry<"blog">;
}

const { entry } = Astro.props;
const { Content } = await render(entry);
---

<section>
  <main>
    <h1>{entry.data.title}</h1>
    <Content />
  </main>
</section>

<style>
  main {
    font-family: "Maven Pro Variable", sans-serif;
  }
</style>
